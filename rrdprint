#!/bin/bash
	rrdtool graph /tmp/x --imgformat PNG\
		--start $1 --end $2 \
		DEF:cpu-e1=/home/stats/data/elephant01-cpu.rrd:total:AVERAGE \
		DEF:cpu-e2=/home/stats/data/elephant02-cpu.rrd:total:AVERAGE \
		DEF:cpu-e3=/home/stats/data/elephant03-cpu.rrd:total:AVERAGE \
		DEF:cpu-e4=/home/stats/data/elephant04-cpu.rrd:total:AVERAGE \
		DEF:cpu-e5=/home/stats/data/elephant05-cpu.rrd:total:AVERAGE \
		CDEF:cpu-en=cpu-e2,cpu-e3,+,cpu-e4,+,cpu-e5,+,4,/ \
		VDEF:cpu-e1a=cpu-e1,AVERAGE \
		VDEF:cpu-ena=cpu-en,AVERAGE \
		PRINT:cpu-e1a:"%lf" \
		PRINT:cpu-ena:"%lf"   \
		DEF:disk-e1r0=/home/stats/data/elephant01-disk-sda.rrd:MBRead:AVERAGE \
		DEF:disk-e1w0=/home/stats/data/elephant01-disk-sda.rrd:MBWritten:AVERAGE \
		DEF:disk-e1r1=/home/stats/data/elephant01-disk-sdb.rrd:MBRead:AVERAGE \
		DEF:disk-e1w1=/home/stats/data/elephant01-disk-sdb.rrd:MBWritten:AVERAGE \
		CDEF:disk-e1t=disk-e1r0,disk-e1w0,+,disk-e1r1,+,disk-e1w1,+ \
		DEF:disk-e2r0=/home/stats/data/elephant02-disk-sda.rrd:MBRead:AVERAGE \
		DEF:disk-e2w0=/home/stats/data/elephant02-disk-sda.rrd:MBWritten:AVERAGE \
		DEF:disk-e2r1=/home/stats/data/elephant02-disk-sdb.rrd:MBRead:AVERAGE \
		DEF:disk-e2w1=/home/stats/data/elephant02-disk-sdb.rrd:MBWritten:AVERAGE \
		CDEF:disk-e2t=disk-e2r0,disk-e2w0,+,disk-e2r1,+,disk-e2w1,+ \
		DEF:disk-e3r0=/home/stats/data/elephant03-disk-sda.rrd:MBRead:AVERAGE \
		DEF:disk-e3w0=/home/stats/data/elephant03-disk-sda.rrd:MBWritten:AVERAGE \
		DEF:disk-e3r1=/home/stats/data/elephant03-disk-sdb.rrd:MBRead:AVERAGE \
		DEF:disk-e3w1=/home/stats/data/elephant03-disk-sdb.rrd:MBWritten:AVERAGE \
		CDEF:disk-e3t=disk-e3r0,disk-e3w0,+,disk-e3r1,+,disk-e3w1,+ \
		DEF:disk-e4r0=/home/stats/data/elephant04-disk-sda.rrd:MBRead:AVERAGE \
		DEF:disk-e4w0=/home/stats/data/elephant04-disk-sda.rrd:MBWritten:AVERAGE \
		DEF:disk-e4r1=/home/stats/data/elephant04-disk-sdb.rrd:MBRead:AVERAGE \
		DEF:disk-e4w1=/home/stats/data/elephant04-disk-sdb.rrd:MBWritten:AVERAGE \
		CDEF:disk-e4t=disk-e4r0,disk-e4w0,+,disk-e4r1,+,disk-e4w1,+ \
		DEF:disk-e5r0=/home/stats/data/elephant05-disk-sda.rrd:MBRead:AVERAGE \
		DEF:disk-e5w0=/home/stats/data/elephant05-disk-sda.rrd:MBWritten:AVERAGE \
		DEF:disk-e5r1=/home/stats/data/elephant05-disk-sdb.rrd:MBRead:AVERAGE \
		DEF:disk-e5w1=/home/stats/data/elephant05-disk-sdb.rrd:MBWritten:AVERAGE \
		CDEF:disk-e5t=disk-e5r0,disk-e5w0,+,disk-e5r1,+,disk-e5w1,+ \
		CDEF:disk-ent=disk-e2t,disk-e3t,+,disk-e4t,+,disk-e5t,+,4,/ \
		VDEF:disk-e1a=disk-e1t,AVERAGE \
		VDEF:disk-ena=disk-ent,AVERAGE \
		PRINT:disk-e1a:"%lf" \
		PRINT:disk-ena:"%lf" \
		DEF:mem-e1u=/home/stats/data/elephant01-mem.rrd:used:AVERAGE \
		DEF:mem-e1b=/home/stats/data/elephant01-mem.rrd:buffers:AVERAGE \
		DEF:mem-e1c=/home/stats/data/elephant01-mem.rrd:cache:AVERAGE \
		CDEF:mem-e1d=mem-e1u,mem-e1b,-,mem-e1c,- \
		DEF:mem-e2u=/home/stats/data/elephant02-mem.rrd:used:AVERAGE \
		DEF:mem-e2b=/home/stats/data/elephant02-mem.rrd:buffers:AVERAGE \
		DEF:mem-e2c=/home/stats/data/elephant02-mem.rrd:cache:AVERAGE \
		CDEF:mem-e2d=mem-e2u,mem-e2b,-,mem-e2c,- \
		DEF:mem-e3u=/home/stats/data/elephant03-mem.rrd:used:AVERAGE \
		DEF:mem-e3b=/home/stats/data/elephant03-mem.rrd:buffers:AVERAGE \
		DEF:mem-e3c=/home/stats/data/elephant03-mem.rrd:cache:AVERAGE \
		CDEF:mem-e3d=mem-e3u,mem-e3b,-,mem-e3c,- \
		DEF:mem-e4u=/home/stats/data/elephant04-mem.rrd:used:AVERAGE \
		DEF:mem-e4b=/home/stats/data/elephant04-mem.rrd:buffers:AVERAGE \
		DEF:mem-e4c=/home/stats/data/elephant04-mem.rrd:cache:AVERAGE \
		CDEF:mem-e4d=mem-e4u,mem-e4b,-,mem-e4c,- \
		DEF:mem-e5u=/home/stats/data/elephant05-mem.rrd:used:AVERAGE \
		DEF:mem-e5b=/home/stats/data/elephant05-mem.rrd:buffers:AVERAGE \
		DEF:mem-e5c=/home/stats/data/elephant05-mem.rrd:cache:AVERAGE \
		CDEF:mem-e5d=mem-e5u,mem-e5b,-,mem-e5c,- \
		CDEF:mem-end=mem-e2d,mem-e3d,+,mem-e4d,+,mem-e5d,+,4,/ \
		VDEF:mem-e1a=mem-e1d,AVERAGE \
		VDEF:mem-ena=mem-end,AVERAGE \
		PRINT:mem-e1a:"%lf" \
		PRINT:mem-ena:"%lf"   \
		DEF:net-e1r0=/home/stats/data/elephant01-net-eth0.rrd:MBRead:AVERAGE \
		DEF:net-e1w0=/home/stats/data/elephant01-net-eth0.rrd:MBWritten:AVERAGE \
		DEF:net-e1r1=/home/stats/data/elephant01-net-eth1.rrd:MBRead:AVERAGE \
		DEF:net-e1w1=/home/stats/data/elephant01-net-eth1.rrd:MBWritten:AVERAGE \
		DEF:net-e1r3=/home/stats/data/elephant01-net-eth3.rrd:MBRead:AVERAGE \
		DEF:net-e1w3=/home/stats/data/elephant01-net-eth3.rrd:MBWritten:AVERAGE \
		CDEF:net-e1t=net-e1r0,net-e1w0,+,net-e1r1,+,net-e1w1,+,net-e1r3,+,net-e1w3,+ \
		DEF:net-e2r0=/home/stats/data/elephant02-net-eth0.rrd:MBRead:AVERAGE \
		DEF:net-e2w0=/home/stats/data/elephant02-net-eth0.rrd:MBWritten:AVERAGE \
		DEF:net-e2r1=/home/stats/data/elephant02-net-eth1.rrd:MBRead:AVERAGE \
		DEF:net-e2w1=/home/stats/data/elephant02-net-eth1.rrd:MBWritten:AVERAGE \
		DEF:net-e2r3=/home/stats/data/elephant02-net-eth3.rrd:MBRead:AVERAGE \
		DEF:net-e2w3=/home/stats/data/elephant02-net-eth3.rrd:MBWritten:AVERAGE \
		CDEF:net-e2t=net-e2r0,net-e2w0,+,net-e2r1,+,net-e2w1,+,net-e2r3,+,net-e2w3,+ \
		DEF:net-e3r0=/home/stats/data/elephant03-net-eth0.rrd:MBRead:AVERAGE \
		DEF:net-e3w0=/home/stats/data/elephant03-net-eth0.rrd:MBWritten:AVERAGE \
		DEF:net-e3r1=/home/stats/data/elephant03-net-eth1.rrd:MBRead:AVERAGE \
		DEF:net-e3w1=/home/stats/data/elephant03-net-eth1.rrd:MBWritten:AVERAGE \
		DEF:net-e3r3=/home/stats/data/elephant03-net-eth3.rrd:MBRead:AVERAGE \
		DEF:net-e3w3=/home/stats/data/elephant03-net-eth3.rrd:MBWritten:AVERAGE \
		CDEF:net-e3t=net-e3r0,net-e3w0,+,net-e3r1,+,net-e3w1,+,net-e3r3,+,net-e3w3,+ \
		DEF:net-e4r0=/home/stats/data/elephant04-net-eth0.rrd:MBRead:AVERAGE \
		DEF:net-e4w0=/home/stats/data/elephant04-net-eth0.rrd:MBWritten:AVERAGE \
		DEF:net-e4r1=/home/stats/data/elephant04-net-eth1.rrd:MBRead:AVERAGE \
		DEF:net-e4w1=/home/stats/data/elephant04-net-eth1.rrd:MBWritten:AVERAGE \
		DEF:net-e4r3=/home/stats/data/elephant04-net-eth3.rrd:MBRead:AVERAGE \
		DEF:net-e4w3=/home/stats/data/elephant04-net-eth3.rrd:MBWritten:AVERAGE \
		CDEF:net-e4t=net-e4r0,net-e4w0,+,net-e4r1,+,net-e4w1,+,net-e4r3,+,net-e4w3,+ \
		DEF:net-e5r0=/home/stats/data/elephant05-net-eth0.rrd:MBRead:AVERAGE \
		DEF:net-e5w0=/home/stats/data/elephant05-net-eth0.rrd:MBWritten:AVERAGE \
		DEF:net-e5r1=/home/stats/data/elephant05-net-eth1.rrd:MBRead:AVERAGE \
		DEF:net-e5w1=/home/stats/data/elephant05-net-eth1.rrd:MBWritten:AVERAGE \
		DEF:net-e5r3=/home/stats/data/elephant05-net-eth3.rrd:MBRead:AVERAGE \
		DEF:net-e5w3=/home/stats/data/elephant05-net-eth3.rrd:MBWritten:AVERAGE \
		CDEF:net-e5t=net-e5r0,net-e5w0,+,net-e5r1,+,net-e5w1,+,net-e5r3,+,net-e5w3,+ \
		CDEF:net-ent=net-e2t,net-e3t,+,net-e4t,+,net-e5t,+,4,/ \
		VDEF:net-e1a=net-e1t,AVERAGE \
		VDEF:net-ena=net-ent,AVERAGE \
		PRINT:net-e1a:"%lf" \
		PRINT:net-ena:"%lf"   \
